{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceRoot}/test",
        "${workspaceRoot}/test/rust.rs",
         "--extensionDevelopmentPath=${workspaceRoot}"
      ],
      "postDebugTask": "npm: build", // Because preLaunch isnt run on restart
      "preLaunchTask": "npm: build"
    }
  ]
}
